0] == 0 and pol_list[1] == 1:\n","#            return 'negative'\n","            return 'negativa'\n","    \n","#************************************************************************\n","#                     word_to_emotions\n","\n","def word_to_emotions(word):\n","    info = word_to_info(word)\n","    if info != None:\n","        emo_info_list = info[2:]\n","        emo_tag_list = emotion_tags[2:]\n","        i = 0\n","        pairs = []\n","        while i < 8:\n","            pairs = pairs + [[emo_tag_list[i], emo_info_list[i]]]\n","            i = i + 1\n","        filtered_pairs = my_list.filter(lambda x: x[1] == 1, pairs)\n","        out = [x[0] for x in filtered_pairs]\n","        if out != []:\n","            return out\n","    \n","# non trova:\n","# felice, licenziato\n","    \n","def parola_emozioni(parola):\n","  return word_to_emotions(parola)\n","\n","#************************************************************************\n","#                     text_to_polarity_info\n","\n","def text_to_polarity_info(input_text):\n","    step1 = word_tokenizer.tokenize(input_text)\n","    step2 = [[x, word_to_polarity(x)] for x in step1]\n","    step3 = my_list.filter(lambda x: None != x[1], step2)\n","    return step3\n","\n","#************************************************************************\n","#                     text_to_emotions_info\n","\n","def text_to_emotions_info(input_text):\n","    step1 = word_tokenizer.tokenize(input_text)\n","    step2 = [[x, word_to_emotions(x)] for x in step1]\n","    step3 = my_list.filter(lambda x: None != x[1], step2)\n","    return step3\n"],"execution_count":31,"outputs":[]},{"cell_type":"code","metadata":{"id":"4gRfjrHYuoSQ","executionInfo":{"status":"ok","timestamp":1618044936705,"user_tz":-120,"elapsed":561,"user":{"displayName":"Alessandro Valitutti","photoUrl":"","userId":"11524397326861718275"}}},"source":["word_tokenizer = RegexpTokenizer('\\w+')\n","\n","#words = word_tokenizer.tokenize(text)\n"],"executi